 .mtn_togg{
	       position: relative;
    left: -7px;
    bottom: -10px;
	color: #fff;
   }
   .toggle_insert{
	        font-size: 18px;
    position: absolute;
    left: -17px;
    bottom: -13px;
    background: #2ECC71;
    color: #fff;
    padding: 1px 11px;
    display: inline-block;
    border-radius: 57px;
    cursor: pointer;
    z-index: 99;
   }
   .hover_toggle{
	    background: #fff;
    position: absolute;
    z-index: 999;
    color: #000;
    border: 1px solid #ddd;
    left: 10px;
    top: -13px;
	display:none;
   }
    .mtn_togg:hover .hover_toggle{
		display:block;
	}
   .hover_toggle small{
	       display: block;
    font-size: 12px;
    font-weight: normal;
    padding:3px 10px;
    border-bottom: 1px solid #ddd;
    cursor: pointer;
	    color: #333;
   }
   .mtn_togg.icono .toggle_insert{
	   left:0;
   }
   .mtn_togg.icono  .hover_toggle{
	left:22px;   
	line-height:inherit;
   }
   
   .card-box.table-responsive{
	padding-bottom:50px;
    background: #fff;   
   }
